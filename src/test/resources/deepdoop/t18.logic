@Entity AP.
@Constructor
byVar[x] = y -> string(x), AP(y).
@Constructor
byFld[x, y] = z -> string(x), string(y), AP(z).
//@Constructor
//byI[x] = z -> int(x), AP(z).

new<<byVar[x] = y as AP>>,
Foo(y, x), Baz(x), BarBaz(y) <- x = "abc".

@entity AP2 -> AP.

new<<byFld[x, y] = z as AP2>>,
Foo(z), Baz2(y, x) <- x = "x2", y = "f1".

