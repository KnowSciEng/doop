Person(x) -> .

cons:Person[x, y] = z -> string(x), int(y), Person(z).
lang:constructor(`cons:Person).

Foo(x, y) -> Person(x), string(y).

new<<cons:Person[y, 1] = z as Person>>,
Foo(z, y) <- y = "G".

///////////////////////////

Animal(x) -> .

@Constructor
cons:Animal[x, y] = z -> string(x), int(y), Animal(z).

Bar(x, y) -> Animal(x), string(y).

new<<cons:Animal[y, 1] = z as Animal>>,
Bar(z, y) <- y = "G".
