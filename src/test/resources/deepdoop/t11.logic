component Foo {
  P(x) <- Z(x).
  Q(x) <- P(x), Q@past(x).
}
as Foo1

component Bar {
  W(x) <- Z(x), P(x).
  A(x), cons:byFoo@past[] = f, B@past(f) <- W(x).
}
as Bar1

Foo1 {*} -> Bar1

component Baz {
  Z(x) <- R(x).
}
as Baz1

component Bar as Bar2
Baz1 {Z} -> Bar2

Bar2 {*} -> .

Bar2:Z@past(x) <- Foo1:P(x).
